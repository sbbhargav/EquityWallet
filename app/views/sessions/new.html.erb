<h1>Sign In</h1>


<%= form_with(url: sessions_path, data: { turbo: false }) do |form| %>

  <div>
		<%= form.label :email%><br>
		<%= form.text_field :email %>
	</div>

	<div>
		<%= form.label :password%><br>
		<%= form.text_field :password %>
	</div>

	<div>
		<%= form.submit 'Log In' %>
	</div>

	
<% end %>